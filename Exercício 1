int[] numeros = new int[5];

for (int i = 0; i < 5; i++)
{
  Console.WriteLine("Digite o número " + (i + 1) + ":");
  numeros[i] = Convert.ToInt32(Console.ReadLine()); 
}

Console.WriteLine("Os nnúmeros digitados: ");

foreach (int numero in numeros) 
{
  Console.WriteLine(numero);
}
Console.ReadKey();
